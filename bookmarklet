javascript:(function() {
var pattern = new RegExp("_aid");
var website_links = document.getElementsByTagName("a"); 
for (var i = 0; i < website_links.length; i++) { 
	var match = website_links[i].getAttribute("href").match(pattern); 
	if (match) { 
		website_links[i].setAttribute("href", website_links[i].getAttribute("href") + "?utm_medium=instant-message");
		console.log(website_links[i].getAttribute("href"));
	} 
}
}
)();
